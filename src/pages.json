{
	"easycom": {
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ 
		//pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/home/home",
			"style": {
				"navigationBarTitleText": "财富自由计算器"
			}
		},
		{
			"path": "pages/calculator/calculator",
			"style": {
				"navigationBarTitleText": "财富自由计算器"
			}
		},
		{
			"path": "pages/charts/charts",
			"style": {
				"navigationBarTitleText": "图表实例"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"color": "#909399",
		"selectedColor": "#303133",
		"backgroundColor": "#FFFFFF",
		"borderStyle": "black",
		"list": [{
				"pagePath": "pages/home/home",
				"iconPath": "static/icon/component.png",
				"selectedIconPath": "static/icon/component_select.png",
				"text": "计算器"
			},
			{
				"pagePath": "pages/calculator/calculator",
				"iconPath": "static/icon/js.png",
				"selectedIconPath": "static/icon/js_select.png",
				"text": "工具"
			},
			{
				"pagePath": "pages/charts/charts",
				"iconPath": "static/icon/template.png",
				"selectedIconPath": "static/icon/template_select.png",
				"text": "图表"
			}
		]
	}
}
